{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2022", "dom"],
    "types": ["cypress", "node", "cypress-xpath"],
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strictNullChecks": true
  },
  "include": ["**/*.ts", "cypress/support/e2e", "cypress/support/commands"]
}
